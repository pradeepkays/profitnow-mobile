{
  "name": "ProfitNow",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "keystore": "cd android/app && keytool -genkey -v -keystore keystore.jks -alias profitnow -keyalg RSA -keysize 2048 -validity 9125 && cd ..",
    "keystore:pem": "cd android/app && keytool -export -rfc -alias upload -file upload_certificate.pem -keystore keystore.jks && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android:aab": "cd android && ./gradlew bundleRelease && cd ..",
    "dep-check": "rnx-align-deps",
    "postinstall": "patch-package"
  },
